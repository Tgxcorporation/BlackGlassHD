			<width>1920</width>
			<height>1080</height>
			<texture>#header.background</texture>
			<animation effect="fade" time="250" delay="250">WindowOpen</animation>
			<animation effect="fade" time="250" delay="250" condition="window.isvisible(9811)">conditional</animation>
			<animation effect="fade" time="250" delay="250" condition="window.isvisible(96742)">conditional</animation>
			<animation effect="fade" time="250" delay="250" condition="window.isvisible(20130603)">conditional</animation>
